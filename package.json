    "@types/axios": "^0.9.36",
    "@types/jest": "^29.5.14",
    "@types/jsonpath": "^0.2.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "jsonpath": "^1.1.1",
    "rxjs": "^7.8.1"
  }
