    "publish": "npm publish --access restricted",
    "test": "jest src/tests --detectOpenHandles"
  },
  "keywords": [
    "clean-architecture",
    "typescript",
    "gateway"
  ],
  "author": "denis.bruns@btc-embedded.com",
  "license": "MIT",
