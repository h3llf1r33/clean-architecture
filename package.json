{
  "name": "@denis_bruns/clean-architecture",
  "version": "1.0.9",
  "description": "Clean architecture implementation",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "dist/types"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/lib --ext .ts",
    "clean": "rimraf dist",
